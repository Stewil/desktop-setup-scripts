alias ll='ls -la --group-directories-first --color=auto -h'
alias ..='cd ..'
alias cd..='cd ..'
alias .2='cd ../..'
alias .3='cd ../../..'
alias .4='cd ../../../..'
alias .5='cd ../../../../..'
alias nf='neofetch'
alias feh='feh -Zdxg 960x540 --scale-down --auto-zoom'
alias getdir='pwd | xclip'
alias godir='cd "$(xclip -o)"'
